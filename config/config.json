{
  "base_image": "quay.io/fedora-ostree-desktops/silverblue",
  "base_image_tag": "43",
  "description": "A laptop image derived from Fedora Silverblue.",
  "github_releases": [
    {
      "project": "twpayne/chezmoi",
      "arch": "x86_64"
    }
  ],
  "packages": {
    "install": [
      "alsa-firmware",
      "code",
      "ffmpegthumbnailer",
      "fira-code-fonts",
      "flatpak-spawn",
      "fuse",
      "fuse-sshfs",
      "gvfs-nfs",
      "heif-pixbuf-loader",
      "input-remapper",
      "intel-vaapi-driver",
      "just",
      "libavcodec",
      "libcamera",
      "libcamera-gstreamer",
      "libcamera-ipa",
      "libcamera-tools",
      "libheif",
      "libva-utils",
      "lshw",
      "mesa-compat-libxatracker",
      "net-tools",
      "openssl",
      "pipewire-libs-extra",
      "pipewire-plugin-libcamera",
      "powerstat",
      "https://proton.me/download/mail/linux/ProtonMail-desktop-beta.rpm",
      "steam-devices",
      "symlinks",
      "tmux",
      "unrar",
      "wireguard-tools",
      "zstd"
    ],
    "remove": [
      {
        "remove": [
          "gnome-tour"
        ]
      },
      {
        "remove": [
          "gstreamer1-plugins-bad-free",
          "gstreamer1-plugins-bad-free-libs"
        ],
        "replace": "gstreamer1-plugins-bad"
      },
      {
        "remove": [
          "gstreamer1-plugins-ugly-free"
        ],
        "replace": "gstreamer1-plugins-ugly"
      }
    ]
  },
  "repos": [
    "https://negativo17.org/repos/fedora-multimedia.repo"
  ]
}
